{
  "name": "admins",
  "fields": {
    "_id": {
      "type": "string",
      "title": "관리자 ID"
    },
    "username": {
      "type": "string",
      "title": "사용자명",
      "required": true
    },
    "email": {
      "type": "string",
      "title": "이메일",
      "required": true
    },
    "password_hash": {
      "type": "string",
      "title": "비밀번호 해시",
      "required": true
    },
    "role": {
      "type": "string",
      "title": "역할",
      "enum": ["super_admin", "admin", "moderator"],
      "default": "admin"
    },
    "is_active": {
      "type": "boolean",
      "title": "활성 상태",
      "default": true
    },
    "last_login": {
      "type": "string",
      "title": "마지막 로그인",
      "format": "datetime"
    },
    "created_at": {
      "type": "string",
      "title": "생성일",
      "format": "datetime"
    },
    "updated_at": {
      "type": "string",
      "title": "수정일",
      "format": "datetime"
    }
  }
}
