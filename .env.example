# ===================================
# Supabase 설정
# ===================================
VITE_SUPABASE_URL=your-supabase-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# ===================================
# 네이버 클라우드 인증 정보
# ===================================
VITE_SMS_ACCESS_KEY=your-ncp-access-key
VITE_SMS_SECRET_KEY=your-ncp-secret-key

# ===================================
# SMS 설정 (네이버 클라우드 SENS)
# ===================================
VITE_SMS_SERVICE_ID=your-sms-service-id
VITE_SMS_FROM_NUMBER=your-phone-number

# ===================================
# 알림톡 설정 (네이버 클라우드 Biz Message)
# ===================================
VITE_NCP_ALIMTALK_SERVICE_ID=your-alimtalk-service-id

# ===================================
# 이메일 설정
# ===================================
VITE_SUPPORT_EMAIL=support@yourdomain.com
VITE_EMAIL_FROM_NAME=Your Company
VITE_COMPANY_NAME=Your Company
VITE_SUPPORT_PHONE=your-phone-number
VITE_WEBSITE_URL=https://yourdomain.com

# ===================================
# Google OAuth 설정
# ===================================
VITE_GOOGLE_CLIENT_ID=your-google-client-id

# ===================================
# 포트원 다날 본인인증 설정
# ===================================
# 포트원 콘솔(https://admin.portone.io)에서 발급받은 정보를 입력하세요
VITE_PORTONE_STORE_ID=store-your-store-id
VITE_PORTONE_CHANNEL_KEY=channel-key-danal

# ===================================
# 개발 환경 설정
# ===================================
VITE_NODE_ENV=development
VITE_API_BASE_URL=http://localhost:3001
